    <script>
$(function() {
    Morris.Area({
        element: 'sensor-chart',
        data: [{x:'1',t:22.50,p:101.543},{x:'2',t:20.50,p:100.00},{x:'3',t:19.66,p:98.754}],
        xkey: 'x',
        ykeys: ['t', 'p'],
        labels: ['Temperature', 'Pressure'],
        pointSize: 2,
        hideHover: 'auto',
        resize: true,
        parseTime: false
    });
    Morris.Donut({
        element: 'temperature-donut',
        data: [{label: "Temperature", value: 20}, {label: "",value: 80}],
        hideHover: 'true',
        resize: true
    });
});
    </script>

